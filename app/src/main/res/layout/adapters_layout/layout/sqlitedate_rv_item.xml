<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/sqlitedata_display_rvitem_sensorName_tv_pre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:text="@string/sensorType"
        android:textSize="15sp"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/sqlitedata_display_rvitem_sensorName_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/sqlitedata_display_rvitem_sensorName_tv_pre"
        app:layout_constraintTop_toTopOf="@id/sqlitedata_display_rvitem_sensorName_tv_pre"
        android:layout_marginLeft="10dp"
        android:text="传感器1"
        android:textSize="15sp"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/sqlitedata_display_rvitem_senseTime_tv_pre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/sqlitedata_display_rvitem_sensorName_tv_pre"
        app:layout_constraintTop_toBottomOf="@id/sqlitedata_display_rvitem_sensorName_tv_pre"
        android:text="@string/sensorTime"
        android:textSize="15sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/sqlitedata_display_rvitem_senseTime_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/sqlitedata_display_rvitem_senseTime_tv_pre"
        app:layout_constraintTop_toTopOf="@id/sqlitedata_display_rvitem_senseTime_tv_pre"
        android:layout_marginLeft="10dp"
        android:text="截止时间"
        android:textSize="15sp"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/sqlitedata_display_rvitem_senseValue_pre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/sqlitedata_display_rvitem_senseTime_tv_pre"
        app:layout_constraintTop_toBottomOf="@id/sqlitedata_display_rvitem_senseTime_tv_pre"
        android:text="@string/senseData"
        android:textSize="15sp"
        android:textColor="@color/black"/>


    <TextView
        android:id="@+id/sqlitedata_display_rvitem_senseValue_1_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/sqlitedata_display_rvitem_senseValue_pre"
        app:layout_constraintTop_toTopOf="@id/sqlitedata_display_rvitem_senseValue_pre"
        android:text="chuanganqizhi1"
        android:layout_marginLeft="10dp"
        android:textSize="15sp"
        android:textColor="@color/colorPrimaryDark"/>
    <TextView
        android:id="@+id/sqlitedata_display_rvitem_senseValue_2_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/sqlitedata_display_rvitem_senseValue_1_tv"
        app:layout_constraintTop_toBottomOf="@id/sqlitedata_display_rvitem_senseValue_1_tv"
        android:text="chuanganqizhi2"
        android:textSize="15sp"

        android:textColor="@color/colorPrimaryDark"/>
    <TextView
        android:id="@+id/sqlitedata_display_rvitem_senseValue_3_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@id/sqlitedata_display_rvitem_senseValue_2_tv"
        app:layout_constraintTop_toBottomOf="@id/sqlitedata_display_rvitem_senseValue_2_tv"
        android:text="chuanganqizhi2"
        android:textSize="15sp"
        android:textColor="@color/colorPrimaryDark"/>


    <View
        android:id="@+id/listview_bottomline"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:layout_constraintTop_toBottomOf="@id/sqlitedata_display_rvitem_senseValue_3_tv"
        android:layout_marginTop="7dp"
        android:background="@color/listview_topline"
        />

</androidx.constraintlayout.widget.ConstraintLayout>